    "template",
    "merge",
    "prosemirror"
  ],
  "author": "Jérémy Lal <kapouer@melix.org>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kapouer/matchdom/issues"
  },
  "homepage": "https://github.com/kapouer/matchdom#readme",
  "devDependencies": {
    "dom4": "^2.0.1",
    "domify": "^1.4.0",
    "global-jsdom": "^7.0.0",
    "jsdom": "^16.4.0",
    "mocha": "^8.2.1"
  },
  "mocha": {
    "require": "global-jsdom/register"
  }

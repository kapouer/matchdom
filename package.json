{
  "name": "matchdom",
  "version": "15.4.0",
  "type": "module",
  "main": "./src/index.js",
  "description": "Extensible declarative template expressions for object models",
  "exports": {
    ".": "./src/index.js",
    "./package.json": "./package.json"
